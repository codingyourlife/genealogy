<Application x:Class="GenealogyApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GenealogyApp"
             xmlns:conv="clr-namespace:GenealogyApp.Converters"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <local:ViewModelLocator x:Key="Locator" />
        <conv:InverseBoolConverter x:Key="inverseBoolConverter" />
        <conv:NullToBooleanConverter x:Key="nullToBooleanConverter" />
        <conv:NewPersonToBooleanConverter x:Key="newPersonToBooleanConverter" />
        <conv:InverseNewPersonToVisbilityConverter x:Key="inverseNewPersonToVisbilityConverter" />
        <conv:NewPersonToVisbilityConverter x:Key="newPersonToVisbilityConverter" />
    </Application.Resources>
</Application>
